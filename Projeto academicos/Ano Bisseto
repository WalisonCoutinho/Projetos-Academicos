#include <stdio.h>

int main() {
    int ano;
    printf("Digite o ano:");
    scanf("%d",&ano);

    if (ano %4 == 0) {
        if ( ano %100 != 0 || ano % 400 == 0) {
            printf("O ano %d e bisseto!\n",ano);
        }else{
            printf("O ano %d nao e bisseto \n",ano);
        }
    }else {
            printf("O ano %d nao e bisseto!\n",ano);
        }
    
return 0;
}